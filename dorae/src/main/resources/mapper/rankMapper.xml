	<select id="list" parameterType="String" resultType="rankVO">
		select
		rank_id, ranking.play_id , rank_genre,rank_area,
		rank_date,ranking,play_name
		from ranking inner join play on ranking.play_id =play.play_id where
		ranking.rank_genre =#{rank_genre} limit 5
	</select>



</mapper>